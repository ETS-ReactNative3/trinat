{
  "name": "trinat-pwa",
  "version": "2.0.0",
  "description": "SIA Trinat local area restaurant in Riga small-scope project. Developed on React, Google Firebase, RaspberryPi and GraphQL API in business purposes",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --progress",
    "eslint": "eslint \"**/*.js\" --ignore-pattern node_modules/",
    "stylelint": "npx stylelint \"./src/app/**/**.scss\"",
    "stylelint:fix": "npx stylelint \"./src/app/**/**.scss\" --fix",
    "test": "npm run eslint ; npm run stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/winniepukki/trinat"
  },
  "keywords": [],
  "author": "Aleksandrs Bogackins",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.12.5",
    "babel-loader": "^8.2.1",
    "copy-webpack-plugin": "^6.3.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-winniepukki-guardian": "^1.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-winniepukki-guidelines": "^2.1.2",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^2.3.0",
    "postcss-loader": "^6.1.1",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "svg-url-loader": "^6.0.0",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "firebase": "^9.1.1",
    "i18n": "^0.13.3",
    "i18next": "^21.2.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.0.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.12.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --cache --no-error-on-unmatched-pattern --ext .js,.ts,.tsx"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@component(.*)$": "<rootDir>/src/app/component$1",
      "^@store(.*)$": "<rootDir>/src/app/store$1",
      "^@util(.*)$": "<rootDir>/src/app/util$1",
      "^@type(.*)$": "<rootDir>/src/app/type$1",
      "^@query(.*)$": "<rootDir>/src/app/query$1"
    }
  }
}
